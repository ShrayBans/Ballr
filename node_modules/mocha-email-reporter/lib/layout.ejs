<html>

  <%- headerContent %>

  <% for (var key in results.suites ) { %>
    <h2><%= key %></h2>
    <% var tests = Object.keys(results.suites[key].tests).map(test => { 
      var t = results.suites[key].tests[test];
      t.title = test; 
      return t;}).sort((a, b) => a.start < b.start ? -1 : 1); %>
    <% tests.forEach((test) => { %>
      <% if (test.pass) { %>
        <dt style="color: green;">âœ” <%= test.title %><span style="font-size: 12px;"> <%= test.duration %> ms </span></dt>
      <% } else { %>
        <dt style="color: tomato;">âœ— <%= test.title %><span style="font-size: 12px;"> <%= test.duration %> ms </span></dt>
      <% } %>
    <% }); %>
  <% } %>
  
  <%- footerContent %>

</html>
